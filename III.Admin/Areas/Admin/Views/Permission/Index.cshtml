@using ESEIM.Utils
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = @ViewData["CrumbPermission"];
    Layout = "~/Areas/Admin/Views/Shared/_Layout_Admin.cshtml";
    var session = HttpContextAccessor.HttpContext.GetSessionUser();
    bool isSubAdmin = session.IsSubAdmin;
}
@section plugin_style {
    <link href="~/lib/assets/global/plugins/bootstrap-table/bootstrap-table.min.css" type="text/css" rel="stylesheet" />
}
@section plugin_script {
    <script src="~/lib/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
}
<div id="contentMain" ng-app="App_ESEIM" ng-controller="Ctrl_ESEIM">
    <div ng-view></div>
</div>
<script src="~/views/admin/permission/controller.js" type="text/javascript"></script>
<script>
    var isSubAdmin=@(isSubAdmin?"true":"false");
</script>